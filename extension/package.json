{
  "name": "devform-ai",
  "displayName": "DevForm AI - Code Generator",
  "description": "AI-powered code generation for any project",
  "version": "0.1.0",
  "publisher": "devform",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:devform.generate",
    "onCommand:devform.status"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "devform.generate",
        "title": "DevForm: Generate Code"
      },
      {
        "command": "devform.status",
        "title": "DevForm: Check Credits"
      }
    ],
    "keybindings": [
      {
        "command": "devform.generate",
        "key": "ctrl+shift+g",
        "mac": "cmd+shift+g"
      }
    ],
    "configuration": {
      "title": "DevForm AI",
      "properties": {
        "devform.apiToken": {
          "type": "string",
          "default": "",
          "description": "Your DevForm API token (get from devform.com)"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "axios": "^1.6.0"
  }
}